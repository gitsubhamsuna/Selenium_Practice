<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Grouping_Suite">

	<groups>
	<run>
		<include name="regression"></include>
		<exclude name="smoke"></exclude>
		<exclude name="sanity"></exclude>
	</run>
	</groups>
  <test thread-count="5" name="Grouping_Test">
    <classes>
      <class name="GroupingTesting.LoginAmazon"/>
      <class name="GroupingTesting.SignupFB"/>
      <class name="GroupingTesting.SignupAmazon"/>
      <class name="GroupingTesting.LoginFB"/>
    </classes>
  </test> <!-- Grouping_Test -->
</suite> <!-- Grouping_Suite -->
